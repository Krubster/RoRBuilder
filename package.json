{
  "name": "ror-career-builder",
  "version": "1.0.0",
  "description": "Web app for users of computer game Return of Reckoning to build and save career specs",
  "main": "",
  "engines": {
    "node": "8.5.0",
    "npm": "5.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "react-scripts build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Natherul/RoRBuilder.git"
  },
  "keywords": [
    "webpack",
    "React",
    "es6",
    "babel",
    "warhammer",
    "Return of Reckoning",
    "Games Workshop",
    "CSS Modules",
    "Redux",
    "React Router"
  ],
  "author": "Matt Richards",
  "license": "MIT",
  "dependencies": {
    "autotrack": "0.6.5",
    "axios": "0.17.1",
    "classnames": "2.2.5",
    "express": "4.16.2",
    "lodash": "4.17.4",
    "query-string": "^5.1.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-hot-loader": "^4.6.3",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-scripts": "^2.1.3",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "redux-promise": "0.5.3"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.1",
    "mini-css-extract-plugin": "^0.5.0",
    "prettier": "^1.15.3",
    "webpack-cli": "^3.1.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint"
  },
  "eslintIgnore": [
    "public"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
